{
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "files.insertFinalNewline": true,
    "editor.tabSize": 4,
    "python.pythonPath": "/usr/bin/python3",
    "python.formatting.provider": "none",
    "files.associations": {
        "unordered_set": "cpp",
        "iostream": "cpp",
        "any": "cpp",
        "array": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "compare": "cpp",
        "functional": "cpp",
        "memory_resource": "cpp",
        "ratio": "cpp",
        "scoped_allocator": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "vector": "cpp",
        "valarray": "cpp",
        "system_error": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "unordered_map": "cpp",
        "initializer_list": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "string_view": "cpp",
        "source_location": "cpp"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    }
}
